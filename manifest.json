{
  "manifest_version": 3,
  "name": "PolicyMe Jira Extension",
  "description": "Make Jira more better",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "worker.js"
  },
  "commands": {
    "next-ticket": {
      "suggested_key": {
        "default": "Alt+Period"
      },
      "description": "Go to the next ticket"
    },
    "previous-ticket": {
      "suggested_key": {
        "default": "Alt+Comma"
      },
      "description": "Go to the previous ticket"
    }
  }
}